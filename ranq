stealAllSelectedBtnStroke.Color = Color3.fromRGB(255, 200, 100)
stealAllSelectedBtnStroke.Thickness = 2
stealAllSelectedBtnStroke.Transparency = 0.3
stealAllSelectedBtnStroke.Parent = stealAllSelectedBtn

