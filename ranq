stealAllBtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
stealAllBtn.BackgroundTransparency = 0.2
stealAllBtn.Text = "üåü ‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô"
stealAllBtn.TextColor3 = Color3.fromRGB(200, 100, 255)
stealAllBtn.TextScaled = true
