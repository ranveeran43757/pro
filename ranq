    ColorSequenceKeypoint.new(0, Color3.fromRGB(150, 50, 200)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(200, 100, 255))
}
stealAllBtnGradient.Rotation = 45
stealAllBtnGradient.Parent = stealAllBtn

-- ‡∏õ‡πâ‡∏≤‡∏¢‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ï‡∏≠‡∏ö‡∏Å‡∏•‡∏±‡∏ö
local feedback = Instance.new("TextLabel")
feedback.Size = UDim2.new(1, -20, 0, 20)
feedback.Position = UDim2.new(0, 10, 0, 690)
feedback.BackgroundTransparency = 1
feedback.Text = "‡∏Ñ‡∏•‡∏¥‡∏Å‡∏™‡πÅ‡∏Å‡∏ô‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ"
feedback.TextColor3 = Color3.fromRGB(255, 255, 100)
feedback.TextScaled = true
feedback.Font = Enum.Font.GothamSemibold
feedback.TextWrapped = true
feedback.TextStrokeTransparency = 0.8
feedback.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
feedback.Parent = mainFrame

print("‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏≠‡∏á‡∏Ñ‡πå‡∏õ‡∏£‡∏∞‡∏Å‡∏≠‡∏ö UI ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÅ‡∏•‡πâ‡∏ß")

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
local function getDistance(pos1, pos2)
    return (pos1 - pos2).Magnitude
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡πÅ‡∏Å‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ
local function scanNearbyPlayers()
    print("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏™‡πÅ‡∏Å‡∏ô‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô...")
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Å‡∏≤‡∏£‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
    char = player.Character
    
    -- ‡∏•‡πâ‡∏≤‡∏á‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡πà‡∏≤
    for _, child in ipairs(playersFrame:GetChildren()) do
        if child:IsA("TextButton") then 
            child:Destroy() 
        end
    end
    
    for _, child in ipairs(itemsFrame:GetChildren()) do
        if child:IsA("TextButton") then 
            child:Destroy() 
        end
    end
    
    nearbyPlayers = {}
    local scanRange = tonumber(rangeSlider.Text) or 50
    scanRange = math.clamp(scanRange, 10, 200)
    rangeLabel.Text = "‡∏£‡∏∞‡∏¢‡∏∞‡∏™‡πÅ‡∏Å‡∏ô: " .. scanRange .. " ‡∏´‡∏ô‡πà‡∏ß‡∏¢"
    
    if not char or not char:FindFirstChild("HumanoidRootPart") then
        feedback.Text = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì"
        return
    end
    
    local myPos = char.HumanoidRootPart.Position
    
    -- ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ
    for _, otherPlayer in ipairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            local otherChar = otherPlayer.Character
            local otherRoot = otherChar:FindFirstChild("HumanoidRootPart")
            
            if otherRoot then
                local distance = getDistance(myPos, otherRoot.Position)
                
                if distance <= scanRange then
                    table.insert(nearbyPlayers, {
                        player = otherPlayer,
                        distance = distance,
                        character = otherChar
                    })
                end
            end
        end
    end
    
    print("‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ:", #nearbyPlayers, "‡∏Ñ‡∏ô")
    
    if #nearbyPlayers == 0 then
        feedback.Text = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏ô‡∏£‡∏∞‡∏¢‡∏∞ " .. scanRange .. " ‡∏´‡∏ô‡πà‡∏ß‡∏¢"
        return
    end
    
    -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏á‡∏ï‡∏≤‡∏°‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
    table.sort(nearbyPlayers, function(a, b) return a.distance < b.distance end)
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
    for i, playerData in ipairs(nearbyPlayers) do
        local btn = Instance.new("TextButton")
        btn.Parent = playersFrame
        btn.LayoutOrder = i
        btn.Size = UDim2.new(1, -10, 0, 40)
        btn.Position = UDim2.new(0, 5, 0, 0)
        btn.Text = "üë§ " .. playerData.player.Name .. " (" .. math.floor(playerData.distance) .. "‡∏°.)"
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.TextScaled = true
        btn.Font = Enum.Font.GothamSemibold
        btn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        btn.BackgroundTransparency = 0.3
        btn.BorderSizePixel = 0
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 8)
        btnCorner.Parent = btn
        
        local btnStroke = Instance.new("UIStroke")
        btnStroke.Color = Color3.fromRGB(100, 150, 255)
        btnStroke.Thickness = 1
        btnStroke.Transparency = 0.7
        btnStroke.Parent = btn
        
        btn.MouseButton1Click:Connect(function()
            selectedPlayer = playerData
            print("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô:", playerData.player.Name)
            
            -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏µ‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            for _, c in ipairs(playersFrame:GetChildren()) do
                if c:IsA("TextButton") then 
                    c.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    c.BackgroundTransparency = 0.3
                    c:FindFirstChild("UIStroke").Color = Color3.fromRGB(100, 150, 255)
                    c:FindFirstChild("UIStroke").Transparency = 0.7
                end
            end
            
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
            btn.BackgroundColor3 = Color3.fromRGB(100, 200, 255)
            btn.BackgroundTransparency = 0.1
            btnStroke.Color = Color3.fromRGB(150, 220, 255)
            btnStroke.Transparency = 0.3
            
            -- ‡πÅ‡∏™‡∏î‡∏á‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
            showPlayerItems(playerData)
        end)
    end
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡∏ô‡∏≤‡∏î Canvas
    playersFrame.CanvasSize = UDim2.new(0, 0, 0, playersLayout.AbsoluteContentSize.Y + 10)
    feedback.Text = "‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô: " .. #nearbyPlayers .. " ‡∏Ñ‡∏ô"
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÅ‡∏™‡∏î‡∏á‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local function showPlayerItems(playerData)
    print("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏î‡∏π‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏Ç‡∏≠‡∏á:", playerData.player.Name)
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Å‡∏≤‡∏£‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏£‡∏±‡∏ö‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
    if playerData.player == player then
        playerData.character = player.Character
    end
    
    -- ‡∏•‡πâ‡∏≤‡∏á‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡πÄ‡∏Å‡πà‡∏≤
    for _, child in ipairs(itemsFrame:GetChildren()) do
        if child:IsA("TextButton") then 
            child:Destroy() 
        end
    end
    
    local items = {}
    local searchTerm = searchBox.Text:lower()
    
    -- ‡∏´‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏à‡∏≤‡∏Å Backpack
    if playerData.player.Backpack then
        for _, tool in ipairs(playerData.player.Backpack:GetChildren()) do
            if tool:IsA("Tool") then
                local toolName = tool.Name:lower()
                if searchTerm == "" or toolName:find(searchTerm) then
                    table.insert(items, {tool = tool, location = "‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤"})
                end
            end
        end
    end
    
    -- ‡∏´‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ñ‡∏∑‡∏≠ - ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Å‡∏≤‡∏£‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
    local currentChar = playerData.player.Character
    if currentChar then
        for _, tool in ipairs(currentChar:GetChildren()) do
            if tool:IsA("Tool") then
                local toolName = tool.Name:lower()
                if searchTerm == "" or toolName:find(searchTerm) then
                    table.insert(items, {tool = tool, location = "‡∏°‡∏∑‡∏≠"})
                end
            end
        end
    end
    
    print("‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏Ç‡∏≠‡∏á", playerData.player.Name .. ":", #items, "‡∏ä‡∏¥‡πâ‡∏ô")
    
    if #items == 0 then
        if searchTerm ~= "" then
            feedback.Text = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö '" .. searchBox.Text .. "' ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö " .. playerData.player.Name
        else
            feedback.Text = playerData.player.Name .. " ‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°"
        end
        return
    end
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏ï‡πà‡∏•‡∏∞‡πÑ‡∏≠‡πÄ‡∏ó‡∏°
    for i, itemData in ipairs(items) do
        local btn = Instance.new("TextButton")
        btn.Parent = itemsFrame
        btn.LayoutOrder = i
        btn.Size = UDim2.new(1, -10, 0, 40)
        btn.Position = UDim2.new(0, 5, 0, 0)
        local locationIcon = itemData.location == "‡∏°‡∏∑‡∏≠" and "‚úã" or "üéí"
        btn.Text = locationIcon .. " " .. itemData.tool.Name .. " (" .. itemData.location .. ")"
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.TextScaled = true
        btn.Font = Enum.Font.GothamSemibold
        btn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        btn.BackgroundTransparency = 0.3
        btn.BorderSizePixel = 0
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 8)
        btnCorner.Parent = btn
        
        local btnStroke = Instance.new("UIStroke")
        btnStroke.Color = Color3.fromRGB(255, 150, 100)
        btnStroke.Thickness = 1
        btnStroke.Transparency = 0.7
        btnStroke.Parent = btn
        
        btn.MouseButton1Click:Connect(function()
            selectedTool = itemData
            print("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÑ‡∏≠‡πÄ‡∏ó‡∏°:", itemData.tool.Name)
            
            -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏µ‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            for _, c in ipairs(itemsFrame:GetChildren()) do
                if c:IsA("TextButton") then 
                    c.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    c.BackgroundTransparency = 0.3
                    c:FindFirstChild("UIStroke").Color = Color3.fromRGB(255, 150, 100)
                    c:FindFirstChild("UIStroke").Transparency = 0.7
                end
            end
            
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
            btn.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
            btn.BackgroundTransparency = 0.1
            btnStroke.Color = Color3.fromRGB(255, 150, 150)
            btnStroke.Transparency = 0.3
            feedback.Text = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å: " .. itemData.tool.Name .. " ‡∏à‡∏≤‡∏Å " .. playerData.player.Name
        end)
    end
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡∏ô‡∏≤‡∏î Canvas
    itemsFrame.CanvasSize = UDim2.new(0, 0, 0, itemsLayout.AbsoluteContentSize.Y + 10)
    feedback.Text = playerData.player.Name .. " ‡∏°‡∏µ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°: " .. #items .. " ‡∏ä‡∏¥‡πâ‡∏ô"
end

-- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏à‡∏≤‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local function searchForItem()
    local searchTerm = searchBox.Text:lower()
    if searchTerm == "" then
        feedback.Text = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÉ‡∏™‡πà‡∏ä‡∏∑‡πà‡∏≠‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤"
        return
    end
    
    print("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÑ‡∏≠‡πÄ‡∏ó‡∏°:", searchBox.Text)
    
    -- ‡∏•‡πâ‡∏≤‡∏á‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡πà‡∏≤
    for _, child in ipairs(playersFrame:GetChildren()) do
        if child:IsA("TextButton") then 
            child:Destroy() 
        end
    end
    
    for _, child in ipairs(itemsFrame:GetChildren()) do
        if child:IsA("TextButton") then 
            child:Destroy() 
        end
    end
    
    local foundItems = {}
    local scanRange = tonumber(rangeSlider.Text) or 50
    scanRange = math.clamp(scanRange, 10, 200)
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Å‡∏≤‡∏£‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
    char = player.Character
    
    if not char or not char:FindFirstChild("HumanoidRootPart") then
        feedback.Text = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì"
        return
    end
    
    local myPos = char.HumanoidRootPart.Position
    
    -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏ú‡πà‡∏≤‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
    for _, otherPlayer in ipairs(Players:GetPlayers()) do
        if otherPlayer.Character then
            local otherChar = otherPlayer.Character
            local otherRoot = otherChar:FindFirstChild("HumanoidRootPart")
            local distance = otherRoot and getDistance(myPos, otherRoot.Position) or math.huge
            
            -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÉ‡∏ô Backpack
            if otherPlayer.Backpack then
                for _, tool in ipairs(otherPlayer.Backpack:GetChildren()) do
                    if tool:IsA("Tool") then
                        local toolName = tool.Name:lower()
                        if toolName:find(searchTerm) then
                            table.insert(foundItems, {
                                tool = tool,
                                player = otherPlayer,
                                location = "‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤",
                                distance = distance,
                                character = otherChar
                            })
                        end
                    end
                end
            end
            
            -- ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡πÉ‡∏ô‡∏°‡∏∑‡∏≠
            for _, tool in ipairs(otherChar:GetChildren()) do
                if tool:IsA("Tool") then
                    local toolName = tool.Name:lower()
                    if toolName:find(searchTerm) then
                        table.insert(foundItems, {
                            tool = tool,
                            player = otherPlayer,
                            location = "‡∏°‡∏∑‡∏≠",
                            distance = distance,
                            character = otherChar
                        })
                    end
                end
            end
        end
    end
    
    print("‡∏û‡∏ö", #foundItems, "‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö:", searchBox.Text)
    
    if #foundItems == 0 then
        feedback.Text = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö '" .. searchBox.Text .. "'"
        return
    end
    
    -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏á‡∏ï‡∏≤‡∏°‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
    table.sort(foundItems, function(a, b) return a.distance < b.distance end)
    
    -- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏û‡∏ö
    for i, itemData in ipairs(foundItems) do
        local btn = Instance.new("TextButton")
        btn.Parent = itemsFrame
        btn.LayoutOrder = i
        btn.Size = UDim2.new(1, -10, 0, 40)
        btn.Position = UDim2.new(0, 5, 0, 0)
        
        local distanceText = itemData.distance < math.huge and (" [" .. math.floor(itemData.distance) .. "‡∏°.]") or " [‡πÑ‡∏Å‡∏•]"
        local locationIcon = itemData.location == "‡∏°‡∏∑‡∏≠" and "‚úã" or "üéí"
        btn.Text = locationIcon .. " " .. itemData.tool.Name .. " - " .. itemData.player.Name .. distanceText
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.TextScaled = true
        btn.Font = Enum.Font.GothamSemibold
        btn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        btn.BackgroundTransparency = 0.3
        btn.BorderSizePixel = 0
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 8)
        btnCorner.Parent = btn
        
        local btnStroke = Instance.new("UIStroke")
        btnStroke.Color = Color3.fromRGB(255, 150, 100)
        btnStroke.Thickness = 1
        btnStroke.Transparency = 0.7
        btnStroke.Parent = btn
        
        btn.MouseButton1Click:Connect(function()
            selectedTool = itemData
            selectedPlayer = {
                player = itemData.player,
                distance = itemData.distance,
                character = itemData.character
            }
            print("‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÑ‡∏≠‡πÄ‡∏ó‡∏°:", itemData.tool.Name, "‡∏à‡∏≤‡∏Å", itemData.player.Name)
            
            -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏µ‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
            for _, c in ipairs(itemsFrame:GetChildren()) do
                if c:IsA("TextButton") then 
                    c.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    c.BackgroundTransparency = 0.3
                    if c:FindFirstChild("UIStroke") then
                        c:FindFirstChild("UIStroke").Color = Color3.fromRGB(255, 150, 100)
                        c:FindFirstChild("UIStroke").Transparency = 0.7
                    end
                end
            end
            
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏™‡∏µ‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
            btn.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
            btn.BackgroundTransparency = 0.1
            if btnStroke then
                btnStroke.Color = Color3.fromRGB(255, 150, 150)
                btnStroke.Transparency = 0.3
            end
            feedback.Text = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å: " .. itemData.tool.Name .. " ‡∏à‡∏≤‡∏Å " .. itemData.player.Name
        end)
    end
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Ç‡∏ô‡∏≤‡∏î Canvas
    itemsFrame.CanvasSize = UDim2.new(0, 0, 0, itemsLayout.AbsoluteContentSize.Y + 10)
    feedback.Text = "‡∏û‡∏ö " .. #foundItems .. " ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏ï‡∏£‡∏á‡∏Å‡∏±‡∏ö '" .. searchBox.Text .. "'"
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ç‡πÇ‡∏°‡∏¢‡πÑ‡∏≠‡πÄ‡∏ó‡∏°
local function stealItem()
    print("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢...")
    
    if not selectedPlayer then
        feedback.Text = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô"
        return
    end
    
    if not selectedTool then
        feedback.Text = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏Å‡πà‡∏≠‡∏ô"
        return
    end
    
    local targetTool = selectedTool.tool
    
    if not targetTool or not targetTool.Parent then
        feedback.Text = "‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏´‡∏≤‡∏¢‡πÑ‡∏õ"
        return
    end
    
    print("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ç‡πÇ‡∏°‡∏¢:", targetTool.Name, "‡∏à‡∏≤‡∏Å", selectedPlayer.player.Name)
    
    -- ‡∏•‡∏≠‡∏á‡∏Ç‡πÇ‡∏°‡∏¢‡∏î‡πâ‡∏ß‡∏¢‡∏ß‡∏¥‡∏ò‡∏µ‡∏Å‡∏≤‡∏£‡∏ï‡πà‡∏≤‡∏á‡πÜ
    local success = false
    
    -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 1: Copy ‡πÅ‡∏•‡∏∞‡∏¢‡πâ‡∏≤‡∏¢
    pcall(function()
        local clonedTool = targetTool:Clone()
        clonedTool.Parent = backpack
        success = true
        print("‡∏Ç‡πÇ‡∏°‡∏¢‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡πÇ‡∏î‡∏¢‡πÉ‡∏ä‡πâ‡∏ß‡∏¥‡∏ò‡∏µ Clone")
    end)
    
    -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 2: ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô Parent ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á
    if not success then
        pcall(function()
            targetTool.Parent = backpack
            success = true
            print("‡∏Ç‡πÇ‡∏°‡∏¢‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡πÇ‡∏î‡∏¢‡πÉ‡∏ä‡πâ‡∏ß‡∏¥‡∏ò‡∏µ Parent Change")
        end)
    end
    
    -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 3: ‡πÉ‡∏ä‡πâ FireServer (‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏Å‡∏°‡∏ó‡∏µ‡πà‡∏°‡∏µ RemoteEvent)
    if not success then
        pcall(function()
            for _, obj in ipairs(game:GetDescendants()) do
                if obj:IsA("RemoteEvent") and obj.Name:lower():find("tool") or obj.Name:lower():find("item") then
                    obj:FireServer("give", targetTool.Name, player)
                    success = true
                    print("‡∏™‡πà‡∏á‡∏Ñ‡∏≥‡∏Ç‡∏≠‡∏ú‡πà‡∏≤‡∏ô RemoteEvent:", obj.Name)
                    break
                end
            end
        end)
    end
    
    if success then
        feedback.Text = ("‡∏Ç‡πÇ‡∏°‡∏¢ \"%s\" ‡∏à‡∏≤‡∏Å %s ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!"):format(targetTool.Name, selectedPlayer.player.Name)
        
        -- ‡∏ö‡∏±‡∏á‡∏Ñ‡∏±‡∏ö‡πÉ‡∏´‡πâ‡πÉ‡∏™‡πà‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏Ç‡πÇ‡∏°‡∏¢‡∏°‡∏≤‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô
        pcall(function()
            if humanoid and targetTool.Parent == backpack then
                humanoid:EquipTool(targetTool)
                print("‡πÉ‡∏™‡πà‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏Ç‡πÇ‡∏°‡∏¢‡∏°‡∏≤:", targetTool.Name)
            end
        end)
        
        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
        task.wait(0.5)
        if selectedPlayer then
            showPlayerItems(selectedPlayer)
        end
    else
        feedback.Text = "‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏Ç‡πÇ‡∏°‡∏¢‡πÑ‡∏î‡πâ - ‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô"
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ç‡πÇ‡∏°‡∏¢‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏à‡∏≤‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å
local function stealAllFromSelectedPlayer()
    if not selectedPlayer then
        feedback.Text = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Å‡πà‡∏≠‡∏ô"
        return
    end
    
    print("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏•‡∏∑‡∏≠‡∏Å:", selectedPlayer.player.Name)
    feedback.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ç‡πÇ‡∏°‡∏¢‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏à‡∏≤‡∏Å " .. selectedPlayer.player.Name .. "..."
    
    local totalStolen = 0
    local targetPlayer = selectedPlayer.player
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Å‡∏≤‡∏£‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
    selectedPlayer.character = targetPlayer.Character
    
    -- ‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å Backpack
    if targetPlayer.Backpack then
        for _, tool in ipairs(targetPlayer.Backpack:GetChildren()) do
            if tool:IsA("Tool") then
                pcall(function()
                    local clonedTool = tool:Clone()
                    clonedTool.Parent = backpack
                    totalStolen = totalStolen + 1
                    print("‡∏Ç‡πÇ‡∏°‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", targetPlayer.Name, "(‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤)")
                end)
                
                -- ‡∏•‡∏≠‡∏á‡∏¢‡πâ‡∏≤‡∏¢‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á‡∏´‡∏≤‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ
                if tool.Parent then
                    pcall(function()
                        tool.Parent = backpack
                        totalStolen = totalStolen + 1
                        print("‡∏¢‡πâ‡∏≤‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", targetPlayer.Name, "(‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤)")
                    end)
                end
                
                task.wait(0.01) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô lag
            end
        end
    end
    
    -- ‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å‡∏°‡∏∑‡∏≠
    if selectedPlayer.character then
        for _, tool in ipairs(selectedPlayer.character:GetChildren()) do
            if tool:IsA("Tool") then
                pcall(function()
                    local clonedTool = tool:Clone()
                    clonedTool.Parent = backpack
                    totalStolen = totalStolen + 1
                    print("‡∏Ç‡πÇ‡∏°‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", targetPlayer.Name, "(‡∏°‡∏∑‡∏≠)")
                end)
                
                -- ‡∏•‡∏≠‡∏á‡∏¢‡πâ‡∏≤‡∏¢‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á‡∏´‡∏≤‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ
                if tool.Parent then
                    pcall(function()
                        tool.Parent = backpack
                        totalStolen = totalStolen + 1
                        print("‡∏¢‡πâ‡∏≤‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", targetPlayer.Name, "(‡∏°‡∏∑‡∏≠)")
                    end)
                end
                
                task.wait(0.01) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô lag
            end
        end
    end
    
    feedback.Text = ("‡∏Ç‡πÇ‡∏°‡∏¢ %d ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏à‡∏≤‡∏Å %s ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!"):format(totalStolen, targetPlayer.Name)
    print("‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å", targetPlayer.Name, "‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô! ‡∏£‡∏ß‡∏°‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏Ç‡πÇ‡∏°‡∏¢:", totalStolen)
    
    -- ‡πÉ‡∏™‡πà‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏Ç‡πÇ‡∏°‡∏¢‡∏°‡∏≤‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô
    if totalStolen > 0 then
        pcall(function()
            for _, tool in ipairs(backpack:GetChildren()) do
                if tool:IsA("Tool") and humanoid then
                    humanoid:EquipTool(tool)
                    print("‡πÉ‡∏™‡πà‡πÑ‡∏≠‡πÄ‡∏ó‡∏°:", tool.Name)
                    break
                end
            end
        end)
    end
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
    task.wait(0.5)
    showPlayerItems(selectedPlayer)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ç‡πÇ‡∏°‡∏¢‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏à‡∏≤‡∏Å‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô
local function stealAllItems()
    print("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏°‡∏≤‡∏Å...")
    feedback.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ç‡πÇ‡∏°‡∏¢‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏à‡∏≤‡∏Å‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô..."
    
    local totalStolen = 0
    local scanRange = tonumber(rangeSlider.Text) or 50
    scanRange = math.clamp(scanRange, 10, 200)
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Å‡∏≤‡∏£‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
    char = player.Character
    
    if not char or not char:FindFirstChild("HumanoidRootPart") then
        feedback.Text = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì"
        return
    end
    
    local myPos = char.HumanoidRootPart.Position
    
    -- ‡πÑ‡∏õ‡∏ó‡∏∏‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
    for _, otherPlayer in ipairs(Players:GetPlayers()) do
        if otherPlayer ~= player then
            local distance = math.huge
            
            -- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡∏´‡∏≤‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏°‡∏µ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
            if otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
                distance = getDistance(myPos, otherPlayer.Character.HumanoidRootPart.Position)
            end
            
            -- ‡∏Ç‡πâ‡∏≤‡∏°‡∏´‡∏≤‡∏Å‡∏≠‡∏¢‡∏π‡πà‡πÑ‡∏Å‡∏•‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ (‡πÄ‡∏ß‡πâ‡∏ô‡πÅ‡∏ï‡πà‡πÄ‡∏£‡∏≤‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô‡πÑ‡∏°‡πà‡∏ß‡πà‡∏≤‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á)
            -- ‡∏•‡∏ö‡∏Ñ‡∏≠‡∏°‡πÄ‡∏°‡πâ‡∏ô‡∏ï‡πå 3 ‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡∏ñ‡∏±‡∏î‡πÑ‡∏õ‡∏´‡∏≤‡∏Å‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÑ‡∏°‡πà‡∏ß‡πà‡∏≤‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
            if distance > scanRange * 2 then -- ‡πÉ‡∏ä‡πâ‡∏£‡∏∞‡∏¢‡∏∞‡∏™‡πÅ‡∏Å‡∏ô 2 ‡πÄ‡∏ó‡πà‡∏≤‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏°‡∏≤‡∏Å
                continue
            end
            
            -- ‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å Backpack
            if otherPlayer.Backpack then
                for _, tool in ipairs(otherPlayer.Backpack:GetChildren()) do
                    if tool:IsA("Tool") then
                        pcall(function()
                            local clonedTool = tool:Clone()
                            clonedTool.Parent = backpack
                            totalStolen = totalStolen + 1
                            print("‡∏Ç‡πÇ‡∏°‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", otherPlayer.Name, "(‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤)")
                        end)
                        
                        -- ‡∏•‡∏≠‡∏á‡∏¢‡πâ‡∏≤‡∏¢‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á‡∏´‡∏≤‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ
                        if tool.Parent then
                            pcall(function()
                                tool.Parent = backpack
                                totalStolen = totalStolen + 1
                                print("‡∏¢‡πâ‡∏≤‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", otherPlayer.Name, "(‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤)")
                            end)
                        end
                        
                        task.wait(0.01) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô lag
                    end
                end
            end
            
            -- ‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≤‡∏Å‡∏°‡∏∑‡∏≠
            if otherPlayer.Character then
                for _, tool in ipairs(otherPlayer.Character:GetChildren()) do
                    if tool:IsA("Tool") then
                        pcall(function()
                            local clonedTool = tool:Clone()
                            clonedTool.Parent = backpack
                            totalStolen = totalStolen + 1
                            print("‡∏Ç‡πÇ‡∏°‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", otherPlayer.Name, "(‡∏°‡∏∑‡∏≠)")
                        end)
                        
                        -- ‡∏•‡∏≠‡∏á‡∏¢‡πâ‡∏≤‡∏¢‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á‡∏´‡∏≤‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ
                        if tool.Parent then
                            pcall(function()
                                tool.Parent = backpack
                                totalStolen = totalStolen + 1
                                print("‡∏¢‡πâ‡∏≤‡∏¢:", tool.Name, "‡∏à‡∏≤‡∏Å", otherPlayer.Name, "(‡∏°‡∏∑‡∏≠)")
                            end)
                        end
                        
                        task.wait(0.01) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô lag
                    end
                end
            end
        end
    end
    
    feedback.Text = ("‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏°‡∏≤‡∏Å‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô! ‡∏Ç‡πÇ‡∏°‡∏¢‡∏£‡∏ß‡∏° %d ‡πÑ‡∏≠‡πÄ‡∏ó‡∏°"):format(totalStolen)
    print("‡∏Å‡∏≤‡∏£‡∏Ç‡πÇ‡∏°‡∏¢‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏°‡∏≤‡∏Å‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô! ‡∏£‡∏ß‡∏°‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏Ç‡πÇ‡∏°‡∏¢:", totalStolen)
    
    -- ‡πÉ‡∏™‡πà‡πÑ‡∏≠‡πÄ‡∏ó‡∏°‡∏ó‡∏µ‡πà‡∏Ç‡πÇ‡∏°‡∏¢‡∏°‡∏≤‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÄ‡∏´‡πá‡∏ô
    if totalStolen > 0 then
        pcall(function()
            for _, tool in ipairs(backpack:GetChildren()) do
                if tool:IsA("Tool") and humanoid then
                    humanoid:EquipTool(tool)
                    print("‡πÉ‡∏™‡πà‡πÑ‡∏≠‡πÄ‡∏ó‡∏°:", tool.Name)
                    break
                end
            end
        end)
    end
end

-- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠ events
scanBtn.MouseButton1Click:Connect(function()
    -- ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏õ‡∏∏‡πà‡∏°‡∏î‡πâ‡∏ß‡∏¢‡πÅ‡∏≠‡∏ô‡∏¥‡πÄ‡∏°‡∏ä‡∏±‡∏ô‡∏ó‡∏±‡∏ô‡∏™‡∏°‡∏±‡∏¢
    local tweenInfo = TweenInfo.new(0.1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
    local pressTween = TweenService:Create(scanBtn, tweenInfo, {
        BackgroundTransparency = 0.1,
        Size = UDim2.new(1, -25, 0, 38)
    })
    pressTween:Play()
    
    pressTween.Completed:Connect(function()
        local releaseTween = TweenService:Create(scanBtn, tweenInfo, {
            BackgroundTransparency = 0.2,
            Size = UDim2.new(1, -20, 0, 40)
        })
        releaseTween:Play()
    end)
    
    scanNearbyPlayers()
end)

