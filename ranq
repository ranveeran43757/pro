    ColorSequenceKeypoint.new(0, Color3.fromRGB(200, 150, 50)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 200, 100))
}
stealAllSelectedBtnGradient.Rotation = 45
stealAllSelectedBtnGradient.Parent = stealAllSelectedBtn

-- ‡∏õ‡∏∏‡πà‡∏°‡∏Ç‡πÇ‡∏°‡∏¢‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏à‡∏≤‡∏Å‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏ô
local stealAllBtn = Instance.new("TextButton")
stealAllBtn.Size = UDim2.new(1, -20, 0, 45)
