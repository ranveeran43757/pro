closeBtnStroke.Transparency = 0.6
closeBtnStroke.Parent = closeBtn

closeBtn.MouseButton1Click:Connect(function()
    isUIVisible = false
    mainFrame.Visible = false
    
    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î
    local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
    local tween = TweenService:Create(toggleButton, tweenInfo, {
        Size = UDim2.new(0, 60, 0, 60),
        BackgroundTransparency = 0.3
    })
    tween:Play()
    
    toggleGlow.Transparency = 0.5
    toggleGlow.Color = Color3.fromRGB(100, 200, 255)
    
    print("‡∏õ‡∏¥‡∏î GUI ‡πÅ‡∏•‡πâ‡∏ß")
end)

-- ‡πÅ‡∏ñ‡∏ö‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏£‡∏∞‡∏¢‡∏∞‡∏™‡πÅ‡∏Å‡∏ô
local rangeLabel = Instance.new("TextLabel")
